{
  "name": "massimmersionapproach.com",
  "version": "1.0.0",
  "description": "Mass Immersion Approach website",
  "author": "Shan Rauf",
  "private": true,
  "scripts": {
    "setup": "yarn build && yarn export && yarn serve",
    "dev": "nuxt",
    "dev:network": "nuxt --hostname 0.0.0.0 --port 65305",
    "build": "nuxt-ts build",
    "prod": "yarn build && yarn export",
    "start": "nuxt-ts start",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest --coverage=false"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.3.2",
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "nuxt": "^2.13.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.13.3",
    "@nuxt/typescript-build": "^2.0.0",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@types/jest": "^26.0.4",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.7",
    "vue-jest": "^3.0.4"
  }
}
